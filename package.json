{
  "name": "cosplay2calendar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "e2e": "ng e2e",
    "dev:ssr": "ng run cosplay2calendar:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "prerender": "ng run cosplay2calendar:prerender",
    "analyze": "npm run build:stats && npm run analyze:bundle",
    "build:stats": "ng build --stats-json",
    "analyze:bundle": "webpack-bundle-analyzer ./dist/stats.json",
    "analyze:new": "bundle-stats ./dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.1002.4",
    "@angular/animations": "~10.2.5",
    "@angular/cdk": "^9.2.4",
    "@angular/cli": "^10.2.4",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/localize": "^10.2.5",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/platform-server": "^10.2.5",
    "@angular/router": "~10.2.5",
    "@nguniversal/express-engine": "^9.0.0",
    "express": "^4.18.1",
    "ics": "^2.37.0",
    "oidc-client": "^1.11.5",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "typescript": "^4.0.8",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "bundle-stats": "^3.0.1",
    "webpack-bundle-analyzer": "^4.4.2"
